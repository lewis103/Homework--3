%timeit
%cython #Ran code in cython# 
cpdef perfect (long n): # added cpdef#
  add = 0
	for i in xrange (1, n): # tried using the xrange function vs range#
		if n % i == 0:
			add += i
	if add == n:
		print "you found a perfect number!!"
	else:
		print "That number is not perfect..."

timeit("perfect (900)")
